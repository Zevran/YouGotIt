{
	"name": "You Got It",
	"description": "The best way to get your favorite content automaticaly on StreamNation",
	"author": "sbodovsk & greyrol",
	"version": "0.0.1",
	"manifest_version": 2,
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "icons/icon19-disable.png",
			"38": "icons/icon38-disable.png"
		},
		"default_title": "The best way to get your favorite content automaticaly on StreamNation"
		/*"default_popup": "browseraction/track.html"*/
	},
	"background": {
		"scripts": ["background.js"]
	},
	/*"content_scripts": [
		{
			"matches": [
				"http://"
			],
			"js": ["content.js"]
		}
	],*/
	"permissions": [
		"tabs"	
	],
	"devtools_page": "devtools/devtools.html"
}
